				display block	
				&:hover 
					background rgba(0, 0, 0, 0.34)
			&:first-child a 
					border-top 	1px solid transparent 			
	.searching
		float right	
		div 
			float left
			padding-bottom 19px 
			background #494949
			a 
				color #fff
				display block
				padding 0 20px
				margin-top 15px 
				text-decoration none
				font-weight bold 
				position relative
	.mobile-tauko 
