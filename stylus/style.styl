	.owl-buttons
		position absolute
		top -79px
		right 7px 
		.owl-prev 
			color #000 !important 
			background #000 !important
			opacity 1 !important 
			height 38px
			width 38px
			position relative
			-webkit-border-radius 0 !important 
			border-radius 0 !important
			&:before 
				content ''
				position absolute
				top 8px
				left 15px
				-webkit-border-radius 5px 
				border-radius 5px 
				border-right 10px solid #fff	
				border-bottom 10px solid transparent
				border-top 10px solid transparent
			&:after 
				content ''
				position absolute
				top 8px
				left 18px
				-webkit-border-radius 5px 
				border-radius 5px 
				border-right 10px solid #000	
				border-bottom 10px solid transparent
				border-top 10px solid transparent
			
			&:hover 
				background #900 !important
				&:after 
				  border-right 10px solid #900	
							
			
		.owl-next 
			color #000 !important 
			background #000 !important
			opacity 1 !important 
			height 38px
			width 38px
			position relative
			-webkit-border-radius 0 !important 
			border-radius 0 !important
			&:before 
				content ''
				position absolute
				top 8px
				left 15px
				-webkit-border-radius 5px 
				border-radius 5px 
				border-left 10px solid #fff	
				border-bottom 10px solid transparent
				border-top 10px solid transparent
			&:after 
				content ''
				position absolute
				top 8px
				left 12px
				-webkit-border-radius 5px 
				border-radius 5px 
				border-left 10px solid #000	
				border-bottom 10px solid transparent
				border-top 10px solid transparent
			
			&:hover 
				background #900 !important
				&:after 
				  border-left 10px solid #900	

